	<section name="local">
		<locale name="local.date">d/m/Y</locale>
		<locale name="local.datetime">d/m/Y H:i</locale>
		<locale name="local.time">H:i</locale>
		<locale name="local.format_date">day,month,year</locale>
		<locale name="local.format_datetime">day,month,year,hour,minute,second</locale>
		<locale name="local.format_datetimeshort">day,month,year,hour,minute</locale>
		<locale name="local.format_time">hour,minute,second</locale>
		<locale name="local.format_timeshort">hour,minute</locale>
		<locale name="local.format_ymd">year,month,day</locale>
		<locale name="local.reg_match_date">/^([0-3][0-9])\/([0-1][0-9])\/([0-2][0-9]{3,3})$/</locale>
		<locale name="local.reg_match_datetime">/^([0-3][0-9])\/([0-1][0-9])\/([0-2][0-9]{3,3}) ([0-2][0-9]):([0-5][0-9]):([0-5][0-9])$/</locale>
		<locale name="local.reg_match_datetimeshort">/^([0-3][0-9])\/([0-1][0-9])\/([0-2][0-9]{3,3}) ([0-2][0-9]):([0-5][0-9])$/</locale>
		<locale name="local.reg_match_duration">/^([0-9]*):([0-5][0-9]):([0-5][0-9])$/</locale>
		<locale name="local.reg_match_time">/^([0-2][0-9]):([0-5][0-9]):([0-5][0-9])$/</locale>
		<locale name="local.reg_match_timeshort">/^([0-2][0-9]):([0-5][0-9])$/</locale>
		<locale name="local.reg_match_ymd">/^([0-9]{4})([0-9]{2})([0-9]{2})$/</locale>
		<locale name="local.reg_replace_date"/>
		<locale name="local.reg_replace_datetime">/^([0-9]{2,2}\/[0-9]{2,2}\/[0-9]{4,4} [0-9]{2,2}:[0-9]{2,2})$/ replaceby \1:00</locale>
		<locale name="local.reg_replace_time">/^([0-9]{2,2}:[0-9]{2,2})$/ replaceby \1:00</locale>
