	<section name="mariadb">
		<locale name="mariadb.date">Y-m-d</locale>
		<locale name="mariadb.dateformat.local.date">%d/%m/%Y</locale>
		<locale name="mariadb.dateformat.local.datetime">%d/%m/%Y %H:%i:%S</locale>
		<locale name="mariadb.dateformat.local.time">%H:%i:%S</locale>
		<locale name="mariadb.datetime">Y-m-d H:i:s</locale>
		<locale name="mariadb.duration">H:i:s</locale>
		<locale name="mariadb.reg_match_date">/^([0-9]{3,4})\-([0-9]{1,2})\-([0-9]{1,2})$/</locale>
		<locale name="mariadb.reg_match_datetime">/^([0-9]{3,4})\-([0-9]{1,2})\-([0-9]{1,2}) ([0-2][0-9]):([0-5][0-9]):([0-5][0-9])$/</locale>
		<locale name="mariadb.reg_match_datetimeshort">/^([0-9]{3,4})\-([0-9]{1,2})\-([0-9]{1,2}) ([0-2][0-9]):([0-5][0-9])$/</locale>
		<locale name="mariadb.reg_match_duration">/^([0-9]*):([0-5][0-9]):([0-5][0-9])$/</locale>
		<locale name="mariadb.reg_match_time">/^([0-2][0-9]):([0-5][0-9]):([0-5][0-9])$/</locale>
		<locale name="mariadb.reg_match_timeshort">/^([0-2][0-9]):([0-5][0-9])$/</locale>
		<locale name="mariadb.time">H:i:s</locale>
